%{
    #include <stdio.h>
    extern int yylval;
%}

%%
[0-9]+      { yylval = atoi(yytext); return NUMBER; }
"+"         { return PLUS; }
"-"         { return MINUS; }
"*"         { return TIMES; }
"/"|"รท"     { return DIVIDE; }
\n          { return EOL; }
[ \t\r]     {}

%%

int YYWARP(void){ return 1; }
